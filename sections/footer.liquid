{% assign pt = section.settings.padding_top | prepend: 'pt-' %}
{% assign pb = section.settings.padding_bottom | prepend: 'pb-' %}
{% assign mt = section.settings.margin_top | prepend: 'mt-' %}
{% assign mb = section.settings.margin_bottom | prepend: 'mb-' %}

<footer 
    id="footer" 
    class="overflow-hidden {{ mt }} {{ mb }} {{ section.settings.bg_style }}"
    style="{{ section.settings.bg_color | prepend: 'background-color: ' | append: ';' }}">

    {% if section.settings.show_sep %}
        <hr 
            class="my-0"
            style="{{ section.settings.sep_color | prepend: 'background-color: ' | append: ';' }};">
    {% endif %}

    <div
        id="footer-inner" 
        class="{{ pt }} {{ pb }}">  
        <div class="{% if section.settings.container %}container{% else %}container-fluid{% endif %}">
          <div class="row">
            <!-- Copyright -->
            <div 
                id="footer-copyright" 
                class="col-md-6">
              COPYRIGHT <span
                    id="footer-copyright-copy">
                    &copy; {{ 'now' | date: "%Y" }} 
                </span> Drinkcuzi. ALL RIGHTS RESERVED.
                
                {% if section.settings.show_powered_by %}
                    <span 
                        id="footer-copyright-powered-by"
                        class="ms-2">
                        Powered by
                        <a 
                            href="https://www.shopify.com/?ref=Shopify" 
                            target="_blank">
                            Shopify</a>
                    </span>
                {% endif %}
                {% if section.settings.show_theme_by %}
                    <span 
                        id="footer-copyright-theme-by"
                        class="ms-2">
                        Theme by
                        <a 
                            href="***" 
                            target="_blank">
                            Shopify</a>
                    </span>
                {% endif %}
            </div>

            <!-- Payment icons -->
            {% if section.settings.show_payment_icons %}
            <div 
                        id="footer-payment-icons"
                        class="col-md-6 text-end"><svg width="159" height="28" viewBox="0 0 159 28" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                <rect y="0.208984" width="28.3929" height="27.5816" fill="url(#pattern0)"/>
                <rect x="32.449" y="0.208984" width="28.3929" height="27.5816" fill="url(#pattern1)"/>
                <rect x="65.7092" y="0.208984" width="27.5816" height="27.5816" fill="url(#pattern2)"/>
                <rect x="98.1581" y="0.208984" width="28.3929" height="27.5816" fill="url(#pattern3)"/>
                <rect x="130.607" y="0.208984" width="28.3929" height="27.5816" fill="url(#pattern4)"/>
                <defs>
                <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                <use xlink:href="#image0_33_241" transform="matrix(0.0333333 0 0 0.0343137 0 -0.0147059)"/>
                </pattern>
                <pattern id="pattern1" patternContentUnits="objectBoundingBox" width="1" height="1">
                <use xlink:href="#image1_33_241" transform="matrix(0.0333333 0 0 0.0343137 0 -0.0147059)"/>
                </pattern>
                <pattern id="pattern2" patternContentUnits="objectBoundingBox" width="1" height="1">
                <use xlink:href="#image2_33_241" transform="scale(0.0333333)"/>
                </pattern>
                <pattern id="pattern3" patternContentUnits="objectBoundingBox" width="1" height="1">
                <use xlink:href="#image3_33_241" transform="matrix(0.0333333 0 0 0.0343137 0 -0.0147059)"/>
                </pattern>
                <pattern id="pattern4" patternContentUnits="objectBoundingBox" width="1" height="1">
                <use xlink:href="#image4_33_241" transform="matrix(0.0333333 0 0 0.0343137 0 -0.0147059)"/>
                </pattern>
                <image id="image0_33_241" width="30" height="30" xlink:href="data:image/png;base64,"/>
                <image id="image1_33_241" width="30" height="30" xlink:href="data:image/png;base64,"/>
                <image id="image2_33_241" width="30" height="30" xlink:href="data:image/png;base64,"/>
                <image id="image3_33_241" width="30" height="30" xlink:href="data:image/png;base64,"/>
                <image id="image4_33_241" width="30" height="30" xlink:href="data:image/png;base64,"/>
                </defs>
                </svg>
                </div>
                {% unless shop.enabled_payment_types == empty %}
                    <div 
                        id="footer-payment-icons"
                        class="col-md-6 ">
                        <span class="visually-hidden">
                            {{ 'general.accessibility.payment_methods' | t }}
                        </span>
                        <ul class="list-inline pull-right">
                            {% for type in shop.enabled_payment_types %}
                                <li 
                                    class="list-inline-item m-0" 
                                    data-bs-toggle="tooltip" 
                                    data-bs-placement="top" 
                                    title="{{ type | replace: "_", " " | camelcase }}">
                                    {{ type | payment_type_svg_tag: class: 'icon' }}
                                </li>
                            {% endfor %}
                        </ul>
                        <!-- <span 
                            id="footer-payment-icons-secure"
                            class="opacity-70">
                            {{ 'general.footer.secure_payments' | t }}
                        </span> -->
                    </div>
                {% endunless %}
            {% endif %}
</div>
        </div>

    </div>
</footer>

{% schema %}
{
    "name": "Footer",
    "settings": [
        {
            "type": "header",
            "content": "General settings"
        },
        {
            "type": "checkbox",
            "id": "container",
            "label": "Wrap in a container",
            "default": true
        },
        {
            "type": "color",
            "id": "bg_color",
            "label": "Background color",
            "default": "#343a40"
        },
        {
            "type": "select",
            "id": "bg_style",
            "label": "Background style",
            "options": [
                { "value": "text-body", "label": "Light" },
                { "value": "text-white", "label": "Dark" }
            ],
            "default": "text-white"
        },
        {
            "type": "checkbox",
            "id": "show_powered_by",
            "label": "Show 'powered by' link",
            "default": true
        },
        {
            "type": "checkbox",
            "id": "show_theme_by",
            "label": "Show 'theme by' link",
            "default": true
        },
        {
            "type": "checkbox",
            "id": "show_payment_icons",
            "label": "Show payment icons",
            "default": true
        },
        {
            "type": "header",
            "content": "Separator"
        },
        {
            "type": "checkbox",
            "id": "show_sep",
            "label": "Show separator (border)",
            "default": true
        },
        {
            "type": "color",
            "id": "sep_color",
            "label": "Separator color",
            "default": "#dee2e6"
        },
        {
            "type": "header",
            "content": "Spacing",
            "info": "Based on Bootstrap spacing utility classes [Learn more](https://getbootstrap.com/docs/5.0/utilities/spacing/)"
        },
        {
            "type": "range",
            "id": "padding_top",
            "label": "Padding top",
            "min": 0,
            "max": 12,
            "step": 1,
            "default": 6
        },
        {
            "type": "range",
            "id": "padding_bottom",
            "label": "Padding bottom",
            "min": 0,
            "max": 12,
            "step": 1,
            "default": 2
        },
        {
            "type": "range",
            "id": "margin_top",
            "label": "Margin top",
            "min": 0,
            "max": 12,
            "step": 1,
            "default": 0
        },
        {
            "type": "range",
            "id": "margin_bottom",
            "label": "Margin bottom",
            "min": 0,
            "max": 12,
            "step": 1,
            "default": 0
        }
    ],
    "default": {
        "settings":  {
        }
    }
}
{% endschema %}